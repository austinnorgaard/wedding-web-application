{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "wedding": {
      "appLocation": ".",
      "outputLocation": "build",
      "dataApiLocation": "backend",
      "appBuildCommand": "npm run scss src/Styles/SCSS:src/Styles/CSS && npm run build",
      "apiBuildCommand": "npm run build --if-present",
      "run": "npm run start",
      "appDevserverUrl": "http://localhost:3000",
      "apiDevserverUrl": "http://localhost:8080",
      "githubActionWorkflowLocation": "./.github/workflows/prod-site-upload.yml",
      "appName": "wedding",
      "resourceGroup": "wedding_group"
    }
  }
}